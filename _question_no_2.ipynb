        "#question no:2\n",
        "def add1(n):\n",
        "   d = { 'Square': lambda a : a**2,\n",
        "        'Cube': lambda a : a**3,\n",
        "        'Squareroot': lambda a : a**(1/2)}\n",
        "   sum = 0\n",
        "   for key in d.keys():\n",
        "       sum += d[key](n)\n",
        "   return sum\n",
        "   \n",
        "print(add1(5))"
